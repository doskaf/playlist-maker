<h3><%= @category.name %></h3><br><br>

<h4>This category has <%= @category.playlists.count %> playlists</h4><br>

<% @category.playlists.each do |playlist| %>
    <li><%= link_to playlist.name, playlist_path(playlist) %> by <%= link_to playlist.user.username, user_path(playlist.user) %></li>
<% end %>