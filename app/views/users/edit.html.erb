<h3>Edit User</h3><br><br>
<%= form_for @user do |f| %>

    <%= render partial: "layouts/errors", locals: {object: @user} %>

    <%= f.label :username %>
    <%= f.text_field :username %><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br><br>

    <%= f.submit "Save Changes" %>
<% end %>